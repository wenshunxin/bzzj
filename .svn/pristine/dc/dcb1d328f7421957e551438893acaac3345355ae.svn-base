<!--pages/roomIndex/roomIndex.wxml-->
<view class="wrap">
    <image id="imageBg" style="width:100%;height:100%;" src="../../static/bg.png" alt="" />
    <view class="content">
        <van-nav-bar  left-arrow title="我是房东" custom-class="warp_header" title-class="wrap_title" bind:click-left="onClickLeft" bind:click-right="onClickRight" />
        <view class="main">
            <!-- 标题 -->
            <view class="header" style="flex:1;">
                <view>北臧之家 温馨港湾</view>
                <view>人员信息填报系统</view>
            </view>
            <view style="flex:1;">
                <view class="item"    data-url="roomer"  bindtap="onTap">
                    <image class="" src="../../static/room.png" mode="aspectFit" lazy-load="false" binderror="" bindload=""></image>
                    <view class="" hover-class="none" hover-stop-propagation="false">我的出租房屋信息</view>
                    <image class="" src="../../static/right.png" mode="aspectFit|aspectFill|widthFix" lazy-load="false" binderror="" bindload=""></image>
                </view>
                <view class="item" style="margin-bottom:100rpx;"  data-url="tenantinfo"  bindtap="onTap">
                    <image class="" src="../../static/person.png" mode="aspectFit" lazy-load="false" binderror="" bindload=""></image>
                    <view class="" hover-class="none" hover-stop-propagation="false">我的租户信息</view>
                    <image class="" src="../../static/right.png" mode="aspectFit|aspectFill|widthFix" lazy-load="false" binderror="" bindload=""></image>
                </view>
            </view>
            <view style="flex:1;">
                <van-notice-bar backgroundColor="transparent" color="#fff" speed="50" text="您好，疫情防控人人有责，请您务必准确填报信息，这将是您进京入村的重要凭证。疫情期间请尽量延期返京，返京前务必联系房东是否符合居家隔离条件。" />
            </view>
        </view>
    </view>
</view>
