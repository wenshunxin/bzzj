/**index.wxss**/
.hmain{
    height:300rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
    background: linear-gradient(to right, red, blue);
    -webkit-background-clip: text;
    color: transparent;
}
.hmain view{
    font-size: 38rpx;
    width: 100%;
    font-weight: 600;
    padding: 0 100rpx;
    box-sizing: border-box;
}
.hmain view:last-child{
    text-align: right;
}
.tabs .btn{
    width:80%;
    border-radius: 80rpx;
    margin: 30rpx auto;
    background-color: #1cbbb4;
}
.tabs .btn:active{
    background-color: #1cbbb4;
}
.tabs .btn:focus{
    background-color: #1cbbb4;
}
.header{
     background: linear-gradient(to right, red, blue);
    -webkit-background-clip: text;
    color: transparent;
    text-align: left;
}
image{
    width: 64rpx;
    height:64rpx;
}
.flex{
    display: flex;
    align-items: center;
    align-content: center;
    padding: 0 40rpx;
}
.flex>view{
    display: flex;
    align-items: center;
    align-content: center;
    flex:1;
    background: #9C26B0;
    border:20rpx;
    height:120rpx;
    text-align: center;
    justify-content:center;
    margin:0 20rpx;
    border-radius: 10rpx;
    color: #fff;
    font-size: 28rpx;
}
.flex view text{
    display: block;
    margin-left: 10rpx;
}
.title{
    font-size: 36rpx;
    margin:80rpx 0rpx 20rpx 60rpx;

}

@keyframes remindMessage {
	0% {
		-webkit-transform: translateX(142%);
	}

	100% {
		-webkit-transform: translateX(-142%);
	}
}

.tongzhitext {
	/* margin-right: 80rpx; */
	margin-left: 10rpx;
	overflow: hidden;
	white-space: nowrap;
    text-overflow: ellipsis;
    /* height:80rpx;
    line-height: 80rpx; */
}

.tongzhi-text {
	font-size: 28rpx;
	animation: remindMessage 8s linear infinite;
	width: 100%;
	color: #d09868;
    display: block;
    height:80rpx;
    line-height: 80rpx;
}

.notice-wrap {
	background: #ffebda;
	width: 100%;
	height: 60rpx;
	line-height: 60rpx;
	color: #d09868;
    font-size: 28rpx;
    height:80rpx;
}




.wrap{
    width:100%;
    height:100%;
    position: relative;
    overflow: hidden;
}
.imageBg{
    position: fixed;
    top:0;
    left:0;
    bottom:0;
    right:0;
    width:100%;
    height:100%;
    z-index:-1;
}
.content{
    position: fixed;
    top:0;
    left:0;
    bottom:0;
    right:0;
    width:100%;
    height:100%;
}
.van-nav-bar{
    background:transparent !important;
}
.van-hairline--bottom:after{
    border:0 !important
}
.van-nav-bar__title{
    color: #fff !important;
}
.header{
    flex:1;
    display: flex;
    flex-direction: column;
    align-items: center;
    align-content: center;
}
.header view:first-child{
    margin-top:60rpx;
    font-size:60rpx;
    text-align: center;
    width:100%;
    color:#fff;
    font-weight: 600;
    font-family: HeitiCSEG;
}
.header view:last-child{
    font-size:40rpx;
    color:#fff;
    text-align: center;
    width:100%;
    line-height: 150rpx;
    letter-spacing:6rpx;
}
.item{
    display: flex;
    width:593rpx;
    height:190rpx;
    background-color: #fff;
    align-items: center;
    align-content: center;
    margin:auto;
    margin-bottom: 50rpx;
    border-radius: 20rpx;
    padding: 0 32rpx;
    box-sizing: border-box;
    justify-content: space-around;
}

.item image:last-child{
    width:24rpx;
    height:44rpx;
}
.item view{
    color:#0A548A;
    font-size:34rpx;
    font-weight: 600;
}
.item:last-child{
    margin-bottom: 100rpx;
}
.van-notice-bar__content{
    font-size: 36rpx;
}
.main{
    height:calc(100% - 64px);
    display: flex;
    flex-direction: column;
    justify-content:space-between;
    align-items: center;
    align-content: center;
}
.van-notice-bar__wrap {
    position: absolute !important;
    right:40rpx;
    left:40rpx;
    bottom: 160rpx;
}
.van-nav-bar__left,.van-nav-bar__right{
    display: inline-block !important;
}