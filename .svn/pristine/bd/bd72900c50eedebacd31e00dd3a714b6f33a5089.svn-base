<!--pages/demo/demo.wxml-->

<view class="flexDiv">
    <view >其他家庭成员信息 </view>
    <button size="mini" bindtap="hanldeAdd">+</button>
</view>

<view wx:if="{{isfalg1}}">
        <picker class="" range="{{list}}" data-index="{{index}}" vlaue="{{demoIndex}}" bindchange="bindPickerChange">
        <view class="">{{list[demoIndex]}}</view>
    </picker>
    <view wx:if="{{ list[demoIndex] == '配偶' }}">
        <view class="item">
            <view>姓名:</view>
            <input type="text" placeholder="填写配偶姓名" />
        </view>
        <view class="item">
            <view>手机号:</view>
            <input type="text" placeholder="填写配偶手机号" />
        </view>
        <view class="item">
            <view>二代身份证号:</view>
            <input type="text" placeholder="填写配偶二代身份证号" />
        </view>
    </view>
    <view wx:if="{{list[demoIndex] == '子女'}}">
        <view class="item">
            <view>姓名:</view>
            <input type="text" placeholder="填写子女姓名，多个逗号隔开" />
        </view>
        <view class="item">
            <view>二代身份证号:</view>
            <input type="text" placeholder="填写子女二代身份证号，多个逗号隔开" />
        </view>
    </view>
    <view wx:if="{{list[demoIndex] == '父母' }}">
        <view class="item">
            <view>姓名:</view>
            <input type="text" placeholder="填写父母姓名，多个逗号隔开" />
        </view>
        <view class="item">
            <view>二代身份证号:</view>
            <input type="text" placeholder="填写子女二代身份证号，多个逗号隔开" />
        </view>
    </view>



</view>
